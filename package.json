{
  "name": "@pearson-incubator/coming_soon",
  "version": "1.0.0",
  "description": "",
  "main": "./main.js",
  "scripts": {
    "start": "webpack-dev-server --port 4040 --hot --progress --colors",
    "build": "webpack --prod --progress --colors && mv build/dist.*.js ./main.js",
    "dev": "webpack --dev --progress --colors && mv build/dist.*.js ./main.js",
    "mindev": "webpack --prod --progress --colors && mv build/dist.*.js ./main.js",
    "lint": "node_modules/.bin/esw src",
    "test": "jest --coverage --watch",
    "lint:fix": "npm run lint -- --fix",
    "version": "npm run gen-changelog && git add CHANGELOG.md",
    "ci": "npm test && npm run build",
    "release": "node ./npm_scripts/release.js",
    "verify": "node ./npm_scripts/verify.js"
  },
  "author": "Hema",
  "license": "MIT",
  "repository": {},
  "dependencies": {
    "node-sass": "^4.9.0",
    "prop-types": "15.5.10",
    "react-bootstrap": "^0.32.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "react-intl": "~2.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "**/src/js/TocComponent.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 50,
        "lines": 65,
        "statements": 50
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|demo|fonts|coverage|npm_scripts|translations)/"
    ],
    "testEnvironment": "node",
    "verbose": true
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.0",
    "babel-jest": "21.0.2",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "copy-webpack-plugin": "^4.1.0",
    "css-loader": "0.28.7",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^3.1.2",
    "eslint": "4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-watch": "3.1.0",
    "expect": "1.18.0",
    "expect-jsx": "2.5.1",
    "file-loader": "0.11.2",
    "jest": "^21.2.1",
    "jsdom": "^8.5.0",
    "jsdom-global": "3.0.2",
    "json-loader": "0.5.4",
    "raw-loader": "0.5.1",
    "react-dom": "^15.4.0",
    "react-tap-event-plugin": "2.0.1",
    "react-test-renderer": "^16.2.0",
    "url-loader": "^0.5.9",
    "webpack": "~1.12.9",
    "webpack-dev-server": "~1.14.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-md5-hash": "0.0.5"
  },
  "peerDependencies": {
    "react": "15.6.1"
  }
}
